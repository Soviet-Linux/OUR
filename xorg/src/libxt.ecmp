[info]
name=libxt
version = 1.2.1
url = http://ftp.x.org/pub/individual/lib/libXt-$version.tar.bz2

[description]
X11 toolkit intrinsics library

[dependencies]
libsm 
libice 
libx11 

[download]
wget $URL
tar -xf libXScrnSaver-$version.tar.xz

[install]
	cd libXt-$version

	./configure \
		--prefix=/usr \
                --sysconfdir=/etc \
                --localstatedir=/var \
                --disable-static \
		--with-appdefaultdir=/etc/X11/app-defaults
	make
	make DESTDIR=$BUILD_ROOT install

