[info]
name = libxmu
version = 1.1.4
url = http://ftp.x.org/pub/individual/lib/libXmu-$version.tar.xz

[description]
X11 miscellaneous micro-utility library

[dependencies]
libx11 
libxt 
libxext

[download]
wget $URL
tar -xf libXmu-$version.tar.xz

[install]
	cd libXmu-$version

	./configure \
		--prefix=/usr \
                --sysconfdir=/etc \
                --localstatedir=/var \
                --disable-static
	make
	make DESTDIR=$BUILD_ROOT install

