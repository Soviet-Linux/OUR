[info]
name = libxaw
version = 1.0.14
url = http://ftp.x.org/pub/individual/lib/libXaw-$version.tar.bz2

[description]
X11 Athena Widget library

[dependencies]
libxext 
libxmu 
libxt 
libx11 
libxpm 

[download]
wget $URL
tar -xf libXaw-$version.tar.bz2

[install]
	cd libXaw-$version

	./configure \
		--prefix=/usr \
                --sysconfdir=/etc \
                --localstatedir=/var \
                --disable-static
	make
	make DESTDIR=$BUILD_ROOT install

